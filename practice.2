//求奇数
#include<stdio.h>//方法一

int main()
{
	int i = 1;
	while (i <= 100)
	{
		printf("%d", i);
		i+=2;
	}
	return 0;
}

#include<stdio.h>//方法二

int main()
{
	int i = 1;
	while (i<=100)
	{
		if (i%2!=0)
			printf("%d", i);
		i++;
	}
	return 0;

