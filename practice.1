#include<stdio.h>//求素数，试除法
#include<math.h>
int main()
{
	int i = 0;
	int count = 0;
	//for (i = 101; i <= 200; i+=2)//直接刨去偶数
	for (i = 100; i <= 200; i++)
	{
		int j = 0;
		for (j = 2; j < sqrt(i); j++)//sqrt开平方的库函数
		{
			if (i % j == 0)
			{
				break;
			}
		}
		if (j >sqrt(i))
			count++;
		printf("% d", i);
	}
	printf("\ncount = %d", count);
	return 0;
}

