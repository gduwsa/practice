#include<stdio.h>//青蛙跳台问题
#include<stdlib.h>
int FrogJump(int n)
{
	if (n <= 2)
		return n;
	else
		return FrogJump(n - 1) + FrogJump(n - 2);
}
int Fib(int n)
{
	int a = 1;
	int b = 1;
	int c = 1;
	while (n > 2)
	{
		c = a + b;
		a = b;
		b = c;
		n--;
	}
	return c;
}
int main()
{
	int n = 0;
	printf("输入台阶数\n");
	scanf("%d", &n);
	int counter = FrogJump(n);
	printf("%d\n", counter);
	system("pause");//暂停等待信号（防止一闪而过）
	return 0;
}

