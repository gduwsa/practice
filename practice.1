#include<stdio.h>//汉诺塔问题
int main()
{
	int hanoi(int, char, char, char);
	int n = 0;
	int counter = 0;//计数器
	printf("Input the number of diskes:");//输入磁盘数量
	scanf("%d", &n);//通过键盘给程序中的变量赋值
	counter = hanoi(n,'A', 'B', 'C');//起始柱，辅助柱，目标柱
	return 0;
}
int hanoi(int n, char x, char y, char z)
{
	int move(char, int, char);
	if (n == 1)
		move(x,n,z);
	else
	{
		hanoi(n - 1, x, z, y);
		move(x, n, z);
		hanoi(n - 1, y, x, z);
	}
	return 0;
}
int move(char getone, int n, char putone)
{
	static int k = 1;
	printf("%2d:%3d # %c-->%c\n", k, n, getone, putone);
	if (k++ % 3 == 0)//每三行空一行
		printf("\n");
	return 0;
}

#include<stdio.h>//求n的阶乘
int Fca1(int n)
{
	int i = 0;
	int ret = 1;
	for (i = 1; i <= n; i++)
	{
		ret *= i;
	}
	return ret;
}
int Fca2(int n)
{
	if (n <= 1)
		return 1;
	else
		return n * Fca2(n - 1);
}
int main()
{
	int n = 0;
	int ret = 0;
	scanf("%d", &n);
	ret = Fca2(n);
	printf("%d\n", ret);
	return 0;
}

